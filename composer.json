{
    "name": "vespolina/media",
    "description": "Vespolina media",
    "keywords": ["image", "media"],
    "homepage": "http://vespolina.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina community",
            "homepage": "https://github.com/vespolina/media/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "mikey179/vfsStream": "~1.2"
    },
    "suggest": {
        "knplabs/gaufrette": "When using the Gaufrette adapter",
